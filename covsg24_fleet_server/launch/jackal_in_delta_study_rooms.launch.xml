<?xml version='1.0' ?>
<launch>
  <arg name="use_sim_time" default="false"/>

  <include file="$(find-pkg-share covsg24_fleet_server)/include/rmf_fleet.launch.xml">
    <arg name="fleet_name" value="jackal_fleet"/>
    <arg name="robot_prefix" value="jackal"/>
    <arg name="environment_name" value="delta_study_rooms"/>
    <arg name="use_sim_time" value="$(var use_sim_time)"/>

    <arg name="map_x" value="-6.3"/>
    <arg name="map_y" value="9.5"/>
    <arg name="map_rotation" value="0.283"/>
    <arg name="map_scale" value="1.0"/>
  </include>
</launch>
